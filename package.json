{
    "name": "gerador-cnpj-alfanumerico",
    "version": "1.4.0",
    "type": "module",
    "description": "⚙️ Um gerador e validador de CNPJ alfanumérico 2026, gratuito e online, feito para desenvolvedores e empresas testarem o novo formato da Receita Federal antes da implementação oficial.",
    "main": "dist/src/gerador.js",
    "scripts": {
        "compilar": "tsc -p tsconfig.json",
        "servidor": "live-server --port=5173 --open=index.html --watch=.",
        "desenvolvimento": "concurrently -n TS,SERV \"npm run compilar:assistido\" \"npm run servidor\"",
        "compilar:assistido": "tsc -w -p tsconfig.json",
        "tailwind": "node ./node_modules/tailwindcss/lib/cli.js -i ./src/estilos/tailwind.css -o ./dist/assets/tailwind.min.css -c ./tailwind.config.js --minify",
        "processar": "ts-node src/processos/compilar-tailwind.ts && ts-node src/processos/minificar-css.ts && ts-node src/processos/processar-html.ts",

        "lintar": "eslint . --ext .ts",
        "lintar:correcao": "eslint . --ext .ts --fix",
        "formatar": "prettier --write .",
        "formatar:verificar": "prettier --check ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FernandoBade/gerador-validador-cnpj"
    },
    "keywords": [
        "cnpj",
        "cnpj-alfanumerico",
        "gerador",
        "validador",
        "consultar-cnpj",
        "novo-cnpj",
        "cnpj-2026",
        "modulo11",
        "typescript",
        "nodejs",
        "frontend",
        "tailwindcss",
        "consulta-dados-receita-federal",
        "dados-cnpj",
        "api-cnpj",
        "banco-de-dados"
    ],
    "author": "Fernando Bade",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/FernandoBade/gerador-validador-cnpj"
    },
    "homepage": "https://cnpj.bade.digital/",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "live-server": "^1.2.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "tailwindcss": "^3.4.15"
    }
}
