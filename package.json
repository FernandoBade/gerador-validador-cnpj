{
    "name": "gerador-cnpj-alfanumerico",
    "version": "1.1",
    "description": "⚙️ Um gerador e validador de CNPJ alfanumérico 2026, gratuito e online, pra quem quer testar o novo formato da Receita Federal antes de todo mundo. Feito pra desenvolvedores, analistas de sistema e empresas ajustarem seus bancos de dados, APIs e integrações.",
    "main": "app.ts",
    "scripts": {
        "desenvolvimento": "concurrently -n TS,SERV \"npm:compilar:assistido\" \"npm:servidor\"",
        "servidor": "live-server --port=5173 --open=index.html --watch=index.html,dist .",
        "compilar": "tsc -p tsconfig.json",
        "compilar:assistido": "tsc -w -p tsconfig.json",
        "lintar": "eslint . --ext .ts",
        "lintar:correcao": "eslint . --ext .ts --fix",
        "formatar": "prettier --write .",
        "formatar:verificar": "prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FernandoBade/GeradorDeCNPJAlfanumerico.git"
    },
    "keywords": [
        "cnpj",
        "cnpj-alfanumerico",
        "receita-federal",
        "gerador-cnpj",
        "simulador-cnpj",
        "validador-cnpj",
        "migracao-sistemas",
        "cnpj-brasil",
        "typescript",
        "nodejs",
        "modulo11",
        "cnpj-2026",
        "cnpj-data",
        "brasil-cnpj"
    ],
    "author": "Fernando Bade",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/FernandoBade/GeradorDeCNPJAlfanumerico/issues"
    },
    "homepage": "https://github.com/FernandoBade/GeradorDeCNPJAlfanumerico#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "live-server": "^1.2.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
