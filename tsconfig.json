{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2020",
        "moduleResolution": "Bundler",
        "outDir": "./dist",
        "rootDir": "./",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [ "src/**/*.ts" ],
    "exclude": [ "node_modules"]
}
